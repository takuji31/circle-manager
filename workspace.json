{
  "version": 2,
  "projects": {
    "bot": {
      "root": "apps/bot",
      "sourceRoot": "apps/bot/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/bot",
            "main": "apps/bot/src/main.ts",
            "tsConfig": "apps/bot/tsconfig.app.json",
            "assets": ["apps/bot/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/bot/src/environments/environment.ts",
                  "with": "apps/bot/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "bot:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/bot/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bot"],
          "options": {
            "jestConfig": "apps/bot/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "database": {
      "root": "libs/database",
      "sourceRoot": "libs/database/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/database/**/*.ts"]
          }
        },
        "generate": {
          "executor": "@nx-tools/nx-prisma:generate",
          "options": {
            "schema": "libs/database/prisma/schema.prisma"
          }
        },
        "migrations": {
          "executor": "@nx-tools/nx-prisma:migrations",
          "options": {
            "schema": "libs/database/prisma/schema.prisma"
          }
        },
        "rollback": {
          "executor": "@nx-tools/nx-prisma:rollback",
          "options": {
            "schema": "libs/database/prisma/schema.prisma"
          }
        },
        "seed": {
          "executor": "@nx-tools/nx-prisma:seed",
          "options": {
            "script": "libs/database/src/lib/seed.ts",
            "tsConfig": "libs/database/tsconfig.lib.json"
          }
        }
      }
    },
    "discord": {
      "root": "libs/discord",
      "sourceRoot": "libs/discord/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/discord/**/*.ts"]
          }
        }
      }
    },
    "discord-commands": {
      "root": "apps/discord-commands",
      "sourceRoot": "apps/discord-commands/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/discord-commands",
            "main": "apps/discord-commands/src/main.ts",
            "tsConfig": "apps/discord-commands/tsconfig.app.json",
            "assets": ["apps/discord-commands/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/discord-commands/src/environments/environment.ts",
                  "with": "apps/discord-commands/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "discord-commands:build",
            "watch": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/discord-commands/**/*.ts"]
          }
        }
      }
    },
    "model": {
      "root": "libs/model",
      "sourceRoot": "libs/model/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/model/**/*.ts"]
          }
        }
      }
    },
    "web": {
      "root": "apps/web",
      "sourceRoot": "apps/web",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/web",
            "outputPath": "dist/apps/web"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "web:build",
            "dev": true,
            "port": 3000
          },
          "configurations": {
            "production": {
              "buildTarget": "web:build:production",
              "dev": false,
              "port": null
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "web:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/web/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "nexusgen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn run build:nexus-typegen"
              }
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "web"
}
