    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.4",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/system": "^5.6.0",
    "@mui/x-data-grid": "^5.0.0-beta.2",
    "@next-auth/prisma-adapter": "^1.0.0",
    "@urql/core": "^2.3.6",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-execute": "^1.2.2",
    "apollo-server-micro": "^3.6.2",
    "core-js": "^3.6.5",
    "csv": "^6.0.1",
    "discord-api-types": "^0.32.0",
    "discord.js": "^13.1.0",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.3.2",
    "graphql": "^16.0.0",
    "graphql-scalars": "^1.17.0",
    "immer": "^9.0.6",
    "material-ui-popup-state": "^2.0.0-beta.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^12.1.4",
    "next-auth": "^4.3.1",
    "next-urql": "^3.3.2",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "node-fetch": "3.2.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-is": "^18.1.0",
    "recoil": "^0.7.0",
    "redis": "^4.0.6",
    "regenerator-runtime": "0.13.9",
    "scuid": "^1.1.0",
    "swr": "^1.0.1",
    "ts-node": "^10.2.1",
    "tslib": "^2.0.0",
    "urql": "^2.1.3",
    "yup": "^0.32.11",
    "yup-locale-ja": "^0.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/import-types-preset": "^2.1.15",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typed-document-node": "^2.2.8",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@types/jest": "^27.4.1",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^16.11.31",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.0.2",
    "node-jose-tools": "^1.6.28",
    "prettier": "^2.3.1",
    "prisma": "^3.13.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
