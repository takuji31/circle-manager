{
  "name": "web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "yarn gen",
    "build": "next build",
    "test": "jest",
    "gen": "yarn db:generate && yarn graphql:schemagen --nexusTypegen && yarn graphql:codegen",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "bot:deploy_commands": "yarn ts-node src/tools/deploy-slash-commands.ts",
    "lint": "next lint",
    "bot:start": "yarn ts-node --transpile-only src/bot/index.ts",
    "bot:dev": "yarn ts-node src/bot/index.ts",
    "bot:watch": "yarn ts-node-dev --respawn --exit-child --clear --watch src/ src/bot/index.ts",
    "graphql:schemagen": "yarn ts-node --transpile-only src/graphql",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "send-select-circle-message": "yarn ts-node src/tools/send-select-circle-message",
    "cron:month-survey-reminder": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=run ; yarn ts-node src/cron/month-survey-reminder && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=fail",
    "cron:month-circle-notifier": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=run ; yarn ts-node src/cron/month-circle-notifier && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=fail",
    "cron:start-month-survey": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=run ; yarn ts-node src/cron/start-month-survey && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=fail",
    "cron:month-ranking-maker": "yarn ts-node src/cron/month-ranking-maker",
    "cron:month-ranking-notifier": "yarn ts-node src/cron/month-ranking-notifier",
    "cron:legend-race-reminder": "yarn ts-node src/cron/legend-race-reminder",
    "cron:team-race-reminder": "yarn ts-node src/cron/team-race-reminder",
    "cron:daily-legend-race-reminder": "yarn ts-node src/cron/daily-legend-race-reminder",
    "tools:daily-ranking-maker": "yarn ts-node src/tools/daily-ranking-maker",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"commonjs\"}' ",
    "ts-node-dev": "ts-node-dev -r tsconfig-paths/register --compiler-options '{\"module\":\"commonjs\"}' "
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "private": true,
  "dependencies": {
    "@circle-manager/shared": "1.0.0",
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
