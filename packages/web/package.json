{
  "name": "web",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
    "dev:next": "NODE_OPTIONS='--inspect' next dev",
    "dev:prisma": "pnpm db:studio --browser none",
    "build": "pnpm gen && next build",
    "test": "jest",
    "gen": "pnpm db:generate && pnpm graphql:schemagen --nexusTypegen && pnpm graphql:codegen",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "bot:deploy_commands": "pnpm ts-node src/tools/deploy-slash-commands.ts",
    "lint": "next lint",
    "bot:start": "pnpm ts-node  --transpile-only src/bot/index.ts",
    "bot:dev": "pnpm ts-node src/bot/index.ts",
    "bot:watch": "pnpm ts-node-dev --respawn --exit-child --clear --watch src/ src/bot/index.ts",
    "graphql:schemagen": "pnpm ts-node --transpile-only src/graphql",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "send-select-circle-message": "pnpm ts-node src/tools/send-select-circle-message",
    "cron:month-survey-reminder": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=run ; pnpm ts-node src/cron/month-survey-reminder && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=fail",
    "cron:month-circle-notifier": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=run ; pnpm ts-node src/cron/month-circle-notifier && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=fail",
    "cron:start-month-survey": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=run ; pnpm ts-node src/cron/start-month-survey && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=fail",
    "cron:month-ranking-maker": "pnpm ts-node src/cron/month-ranking-maker",
    "cron:month-ranking-notifier": "pnpm ts-node src/cron/month-ranking-notifier",
    "cron:legend-race-reminder": "pnpm ts-node src/cron/legend-race-reminder",
    "cron:team-race-reminder": "pnpm ts-node src/cron/team-race-reminder",
    "cron:daily-legend-race-reminder": "pnpm ts-node src/cron/daily-legend-race-reminder",
    "tools:daily-ranking-maker": "pnpm ts-node src/tools/daily-ranking-maker",
    "ts-node-dev": "ts-node-dev --compiler-options {\\\"module\\\":\\\"commonjs\\\"} ",
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} "
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "private": true,
