{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "jsx": "preserve",
    "experimentalDecorators": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": false,
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "isolatedModules": true
  },
  "files": [],
  "include": [],
  "exclude": []
}
