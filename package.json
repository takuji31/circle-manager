{
  "name": "web2",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": true,
  "scripts": {
    "postinstall": "remix setup node && yarn generate",
    "prebuild": "yarn generate",
    "build": "run-s build:*",
    "build:css": "yarn generate:css --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "yarn generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate": "run-p generate:*",
    "generate:css": "tailwindcss -i ./app/styles/app.css -o ./app/styles/tailwind.css",
    "generate:db": "prisma generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks @remix-run/serve:remix-serve build",
    "start:bot": "yarn ts-node bot",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test --run\" lint typecheck test:e2e:run",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "send-select-circle-message": "yarn ts-node tools/send-select-circle-message",
    "cron:month-survey-reminder": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=run ; yarn ts-node cron/month-survey-reminder && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=fail",
    "cron:month-circle-notifier": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=run ; yarn ts-node cron/month-circle-notifier && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=fail",
    "cron:start-month-survey": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=run ; yarn ts-node cron/start-month-survey && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=fail",
    "cron:month-ranking-maker": "yarn ts-node cron/month-ranking-maker",
    "cron:month-ranking-notifier": "yarn ts-node cron/month-ranking-notifier",
    "cron:legend-race-reminder": "yarn ts-node cron/legend-race-reminder",
    "cron:team-race-reminder": "yarn ts-node cron/team-race-reminder",
    "cron:daily-legend-race-reminder": "yarn ts-node cron/daily-legend-race-reminder",
    "tools:daily-ranking-maker": "yarn ts-node tools/daily-ranking-maker",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"commonjs\"}' "
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@google-cloud/vision": "^2.4.2",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@js-joda/core": "^5.2.0",
    "@js-joda/locale_ja": "^4.7.0",
    "@js-joda/timezone": "^2.12.0",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "@types/lodash": "^4.14.182",
    "bcryptjs": "^2.4.3",
    "csv-stringify": "^6.0.5",
    "date-fns": "^2.28.0",
    "discord-api-types": "^0.33.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.1",
    "firebase-admin": "^10.1.0",
    "flowbite": "^1.4.5",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "material-ui-confirm": "^3.0.4",
    "material-ui-popup-state": "^3.1.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.0",
    "react-hot-toast": "^2.2.0",
    "recoil": "^0.7.2",
    "redis": "^4.1.0",
    "remix": "^1.4.3",
    "remix-auth": "^3.2.1",
    "remix-auth-discord": "^1.0.0",
    "remove": "^0.1.5",
    "simplebar": "^5.3.6",
    "simplebar-react": "^2.3.7",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@remix-run/dev": "^1.4.1",
    "@remix-run/eslint-config": "^1.4.1",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.23",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.8",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "binode": "^1.0.5",
    "c8": "^7.11.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^3.1.0",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.14.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.10.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
