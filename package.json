{
  "name": "circle-manager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "yarn run db:migrate:deploy && yarn run gen",
    "build": "next build",
    "test": "jest",
    "gen": "yarn run db:generate && yarn run graphql:schemagen && yarn run graphql:codegen",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "bot:deploy_commands": "yarn run ts-node src/tools/deploy-slash-commands.ts",
    "lint": "next lint",
    "bot:start": "yarn run ts-node --transpile-only src/bot/index.ts",
    "bot:dev": "yarn run ts-node src/bot/index.ts",
    "bot:watch": "nodemon",
    "graphql:schemagen": "yarn run ts-node --transpile-only src/graphql",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "send-select-circle-message": "yarn run ts-node src/tools/send-select-circle-message",
    "cron:month-survey-reminder": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=run ; yarn run ts-node src/cron/month-survey-reminder && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=fail",
    "cron:month-circle-notifier": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=run ; yarn run ts-node src/cron/month-circle-notifier && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=fail",
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} "
  },
  "prisma": {
    "seed": "yarn run ts-node src/database/seed.ts"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.4",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/x-data-grid": "^5.0.0-beta.2",
    "@next-auth/prisma-adapter": "^0.5.2-next.19",
    "@prisma/client": "^3.5.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node-fetch": "^2.5.12",
    "apollo-server-micro": "^3.3.0",
    "core-js": "^3.6.5",
    "csv": "^6.0.1",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "framer-motion": "^4",
    "graphql": "^15.6.1",
    "immer": "^9.0.6",
    "material-ui-popup-state": "^2.0.0-beta.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "11.1.2",
    "next-auth": "^4.0.2",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.34.0",
    "proposal-temporal": "^0.9.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.4",
    "regenerator-runtime": "0.13.7",
    "scuid": "^1.1.0",
    "superjson": "^1.8.0",
    "swr": "^1.0.1",
    "ts-node": "^10.2.1",
    "tslib": "^2.0.0",
    "yup": "^0.32.11",
    "yup-locale-ja": "^0.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/import-types-preset": "^2.1.7",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.7",
    "@graphql-codegen/typescript-operations": "2.1.6",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@types/node": "^16.10.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "babel-plugin-superjson-next": "^0.4.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.1",
    "node-jose-tools": "^1.6.28",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.1",
    "prisma": "^3.5.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  }
}
