{
  "name": "circle-manager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "prebuild": "yarn run db:migrate:deploy && yarn run gen && yarn run ts-node libs/database/src/onetime/member_pathname.ts",
    "build": "nx build web && nx build bot",
    "test": "nx test",
    "gen": "nx run database:generate && yarn run graphql:schemagen && yarn run graphql:codegen",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "bot:deploy_commands": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} apps/bot/src/app/deploy-commands.ts",
    "lint": "next lint",
    "bot:dev": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} src/bot/index.ts",
    "bot:watch": "nodemon",
    "graphql:schemagen": "yarn run ts-node --transpile-only apps/web/graphql",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "ts-node": "TS_NODE_PROJECT=tsconfig.node.json ts-node -r tsconfig-paths/register",
    "bot:start": "node dist/apps/bot/main.js"
  },
  "prisma": {
    "seed": "yarn run nx seed database",
    "schema": "libs/database/prisma/schema.prisma"
  },
  "private": true,
