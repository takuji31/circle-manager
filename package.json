{
  "name": "circle-manager",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": true,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "yarn generate:css --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "yarn generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev | yarn pino-pretty",
    "format": "prettier --write .",
    "generate": "run-p generate:*",
    "generate:css": "tailwindcss -i ./app/styles/app.css -o ./app/styles/tailwind.css",
    "generate:db": "prisma generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks @remix-run/serve:remix-serve build",
    "start:bot": "ts-node bot",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test --run\" lint typecheck test:e2e:run",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "send-select-circle-message": "ts-node tools/send-select-circle-message",
    "cron:month-survey-reminder": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=run ; ts-node cron/month-survey-reminder && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=fail",
    "cron:month-circle-notifier": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=run ; ts-node cron/month-circle-notifier && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=fail",
    "cron:start-month-survey": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=run ; ts-node cron/start-month-survey && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=fail",
    "cron:month-ranking-maker": "ts-node cron/month-ranking-maker",
    "cron:month-ranking-notifier": "ts-node cron/month-ranking-notifier",
    "cron:legend-race-reminder": "ts-node cron/legend-race-reminder",
    "cron:team-race-reminder": "ts-node cron/team-race-reminder",
    "cron:daily-legend-race-reminder": "ts-node cron/daily-legend-race-reminder",
    "tools:daily-ranking-maker": "ts-node tools/daily-ranking-maker"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
