    "@js-joda/timezone": "^2.12.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@mui/x-data-grid": "^5.0.0-beta.2",
    "@next-auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^3.10.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node-fetch": "2",
    "@urql/core": "^2.3.6",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-execute": "^1.2.2",
    "apollo-server-micro": "^3.6.2",
    "core-js": "^3.6.5",
    "csv": "^6.0.1",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "framer-motion": "^4",
    "graphql": "^15.8.0",
    "immer": "^9.0.6",
    "material-ui-popup-state": "^2.0.0-beta.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "^12.1.0",
    "next-auth": "^4.0.2",
    "next-urql": "^3.3.2",
    "nexus": "^1.2.0",
    "nexus-prisma": "^0.35.0",
    "node-fetch": "2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "7.21.1",
    "react-is": "^17.0.2",
    "regenerator-runtime": "0.13.7",
    "scuid": "^1.1.0",
    "swr": "^1.0.1",
    "ts-node": "^10.2.1",
    "tslib": "^2.0.0",
    "urql": "^2.1.3",
    "yup": "^0.32.11",
    "yup-locale-ja": "^0.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/import-types-preset": "^2.1.7",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typed-document-node": "^2.2.3",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.6",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.10.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "^27.5.1",
    "node-jose-tools": "^1.6.28",
    "prettier": "^2.3.1",
    "prisma": "^3.10.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "^16.13.2",
    "npm": "^8.1.2",
    "yarn": "^1.22.17"
  }
}
