{
  "name": "circle-manager",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "next start",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "yarn run db:migrate:deploy && yarn run gen",
    "build": "next build",
    "test": "jest",
    "gen": "yarn run db:generate && yarn run graphql:schemagen --nexusTypegen && yarn run graphql:codegen",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "bot:deploy_commands": "yarn run ts-node src/tools/deploy-slash-commands.ts",
    "lint": "next lint",
    "bot:start": "yarn run ts-node --transpile-only src/bot/index.ts",
    "bot:dev": "yarn run ts-node src/bot/index.ts",
    "bot:watch": "yarn run ts-node-dev --respawn --exit-child --clear --watch src/ src/bot/index.ts",
    "graphql:schemagen": "yarn run ts-node --transpile-only src/graphql",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "send-select-circle-message": "yarn run ts-node src/tools/send-select-circle-message",
    "cron:month-survey-reminder": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=run ; yarn run ts-node src/cron/month-survey-reminder && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-survey-reminder?state=fail",
    "cron:month-circle-notifier": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=run ; yarn run ts-node src/cron/month-circle-notifier && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/month-circle-notifier?state=fail",
    "cron:start-month-survey": "curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=run ; yarn run ts-node src/cron/start-month-survey && curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=complete || curl https://cronitor.link/p/2028ed5e5f8544a5adfc7de3d7dd2455/start-month-survey?state=fail",
    "cron:month-ranking-maker": "yarn run ts-node src/cron/month-ranking-maker",
    "cron:month-ranking-notifier": "yarn run ts-node src/cron/month-ranking-notifier",
    "cron:legend-race-reminder": "yarn run ts-node src/cron/legend-race-reminder",
    "tools:daily-ranking-maker": "yarn run ts-node src/tools/daily-ranking-maker",
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} ",
    "ts-node-dev": "ts-node-dev --compiler-options {\\\"module\\\":\\\"commonjs\\\"} "
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "private": true,
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.4",
